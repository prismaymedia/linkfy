{
  "name": "linkfy-server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "nest:dev": "nest start --watch --path tsconfig.json",
    "nest:build": "nest build --path tsconfig.json",
    "nest:start": "NODE_ENV=production node dist/server/src/main.js",
    "nest:check": "tsc -p tsconfig.json --noEmit",
    "nest:db:push": "drizzle-kit push",
    "nest:test": "jest --config jest.config.json",
    "nest:test:e2e": "jest --config jest-e2e.json",
    "nest:test:watch": "jest --watch",
    "nest:test:cov": "jest --coverage",
    "nest:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "nest:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "nest:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "connect-pg-simple": "^10.0.0",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.44.3",
    "drizzle-zod": "^0.8.2",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "googleapis": "^156.0.0",
    "memorystore": "^1.6.7",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.18.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cors": "^2.8.19",
    "@types/express": "5.0.3",
    "@types/express-session": "^1.18.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.2.1",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.5.13",
    "drizzle-kit": "^0.31.4",
    "jest": "30.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.4.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
